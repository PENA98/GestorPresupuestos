  <aside class="main-sidebar">
    <!-- sidebar: style can be found in sidebar.less -->
    <section class="sidebar">
      <!-- Sidebar user panel -->
      <div class="user-panel">
        <div class="pull-left image">
          <img src="/templates/dist/img/user2-160x160.jpg" class="img-circle" alt="User Image">
        </div>
        <div class="pull-left info">
          <p>{{user.name}}</p>
          <a href="#"><i class="fa fa-circle text-success"></i> En linea</a>
        </div>
      </div>
      <!-- sidebar menu: : style can be found in sidebar.less -->
      <ul class="sidebar-menu" data-widget="tree">
        <li class="header">Navegación principal</li>
        <li class="active treeview">
          <a href="#">
            <i class="fa fa-home"></i> <span>Inicio</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <ul class="treeview-menu">
            <li class="active"><a href="/home"><i class="fa fa-list"></i> Vista general</a></li>
          </ul>
        </li>

        <li >
          <a href="/transactions">
            <i class="fa fa-table"></i> <span>Transacciones</span>
          </a>
        </li>
        
        <li class="treeview">
          <a href="#">
            <i class="fa fa-university"></i>
            <span>Cuentas</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <ul class="treeview-menu">
            <li><a href="/accounts"><i class="fa fa-address-book"></i> Mis cuentas</a></li>
            <li><a href="/create_account"><i class="fa fa-plus-square"></i> Agregar cuenta</a></li>
          </ul>
        </li>

        <li class="treeview">
          <a href="#">
            <i class="fa fa-credit-card"></i> <span>Tarjetas de credito</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <ul class="treeview-menu">
            <li><a href="/create_card"><i class="fa fa-plus-square"></i>Crear</a></li>
            <li><a href="/cards"><i class="fa fa-eye"></i>Ver </a></li>
          </ul>
        </li>

        <li class="treeview">
          <a href="#">
            <i class="fa fa-pie-chart"></i>
            <span>Graficos</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <ul class="treeview-menu">
            <li><a href="/graficos"><i class="fa fa-circle-o"></i> ChartJS</a></li>
          </ul>
        </li>

        <li class="treeview">
          <a href="#">
            <i class="fa fa-inbox"></i> <span>Ahorros</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <ul class="treeview-menu">
            <li><a href="/savings"><i class="fa fa-circle-o"></i>Ver ahorros</a></li>
            <li><a href="/new_saving"><i class="fa fa-circle-o"></i>Crear ahorro </a></li>
          </ul>
        </li>
        
        
      </ul>
    </section>
    <!-- /.sidebar -->
  </aside>

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <section class="content">
    {{{body}}}

    <!--FAB-->
    <div class="inner-fabs">
      <div class="fab round green" id="fab4" data-toggle="modal" data-target="#squarespaceModal" data-tooltip="Agregar ingreso"><i class="ion ion-arrow-graph-up-right"></i></div>
      <div class="fab round blue" id="fab3" data-toggle="modal" data-target="#modalExpense"data-tooltip="Agregar gasto"><i class="ion ion-arrow-graph-down-right"></i></div>
      <div class="fab round yellow" id="fab2" data-toggle="modal" data-target="#modalCard" data-tooltip="Crear gasto de la tarjeta"><i class="fa fa-credit-card"></i></div>
      <div class="fab round otherblue" id="fab5" data-toggle="modal" data-target="#modalSavings" data-tooltip="Ahorros"><i class="fa fa-inbox"></i></div>
    </div>
    <div class="fab round" id="fab1"><i class="material-icons" id="fabIcon">add</i></div>
      <!--/FAB-->
      
    </section>
    
    <!-- /.content -->
  </div>
  <!-- /.control-sidebar -->
  <!-- Add the sidebar's background. This div must be placed
       immediately after the control sidebar -->
  <div class="control-sidebar-bg"></div>
</div>
